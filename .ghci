:set -Wall -fno-warn-name-shadowing -Wcompat
:set -Wincomplete-record-updates -Wincomplete-uni-patterns -Wredundant-constraints

:set -isrc

:load CCC

:set prompt "\x03BB> "
:set prompt-cont "\x03BB| "
:!cls

:{
:def go \top -> return $ unlines $
    [ ":set -fno-warn-type-defaults"
    , ":m " ++ top
    , ":show imports" ]
:}
